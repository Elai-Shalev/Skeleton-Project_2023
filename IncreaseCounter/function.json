{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "type": "signalR",
      "name": "signalRMessages",
      "hubName": "IotSignalRElamar",
      "connection": "AzureSignalRConnectionString",
      "direction": "out"
    },
    {
      "type": "eventHub",
      "name": "testEventHub",
      "eventHubName": "ioteventhubelamar",
      "connection": "MyEventHubSendAppSetting",
      "direction": "out"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}